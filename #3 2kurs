#include<iostream>
using namespace std;

class Arr {
public:
	int*arr;
	int k;
	int max;
	Arr(int number = 0)
	{
		int i = 0, n = number;
		while (n > 0)
		{
			n = n / 10;

			++i;
		}
		arr = new int[i];
		k = i;
		for (int in=i; in > 0;--in)
		{
			arr[in - 1] = number % 10;
			number = number / 10;
			
		}
	}

		void sort()
		{
		
				for (int j=0; j<k; ++j)
				{
					for (int i = 0; i < k; ++i) 
					{
						if (arr[i] > arr[i + 1]) swap(arr[i], arr[i + 1]);
					}
				}		
			
		}

		friend ostream& operator<<(ostream &os, Arr a) 
		{
			for (int i = 1; i < a.k+1; ++i)
			{
				os << a.arr[i]<<" ";		
			}
			os << "\n";
			return os;
		}
		
		
};



	
void main() 
{
	Arr a(15342987);
	a.sort();
	cout << a;

}
