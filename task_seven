#include"iostream"
using namespace std;
int main()
{
	setlocale(LC_ALL, "ukr");
	int n, x, y;
	cout << "Введiть число n :";
	cin >> n;
	for (int x = 0;x < n;++x){
		for (int y = 0;y < n;y++)
		{
			if (n == pow(x,2) + pow(y,2))
			{
				cout << x << ' ' << y << '\t' << endl;
			}
		}
	}
	system("pause");
	return 0;
}
