using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Threading;
using System.Diagnostics;
using System.ComponentModel;




namespace ConsoleApplication3
{
    class Program
    {
        static void Main(string[] args)
        {
            int n;
            Console.WriteLine("Enter number of words");
            n = Convert.ToInt32(Console.ReadLine());
            String[] array = new string[n];
            for (int i = 0; i < n; i++) {
                array[i] = Console.ReadLine();
            }
            for (int i = 0; i < n; i++) {
                String someStr = someFunk(array[i]);
                if (!(someStr.Equals("")))
                {
                    array[i]=someStr;
                }
            }
            foreach (String temp in array) {
                Console.WriteLine(temp);
            }

        }

        static String someFunk(String str) {
            string result=str;
            char one= result[result.Length - 1];
            char two = result[result.Length - 2];
            for (int i = 0; i < result.Length; i++) {
                if (result[i].Equals(one) || result[i].Equals(two)) {
                    result=result.Remove(i, 1);
                    i--;
                }
            }
            return result;
        }
    }
}
