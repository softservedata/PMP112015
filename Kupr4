a=str(input('a='))
for i in range(len(a)-1):
    if (a[i]=='.')and(a[i+1].islower()==True):
        a=a[:i+1]+a[i+1].upper()+a[i+2:]            
for i in range(len(a)-3):
    if (a[i]==' ')and(a[i+1].isupper()==True)and(a[i+2].isupper()==True)and(a[i+3].islower()==True):
        a=a[:i+2]+a[i+2].lower()+a[i+3:]
print(a)
