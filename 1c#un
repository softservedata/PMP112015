using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication114
{
    class Program
    {
        static void Main(string[] args)
        {
            int s = int.Parse(Console.ReadLine());
            for (int i =0 ; i < s; ++i)
            {
                for (int j = 1; j < s+1; ++j)
                {
                    Console.Write(i + j);
                }
                Console.Write("\n");
            }
        }
    }
}
