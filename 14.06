#ifndef MYSTACK_H
#define MYSTACK_H
#include<iostream>
#include"list"
#include"string"
using namespace std;

class MyStack:private list<string>
{
public:
	MyStack()
	{}
	void push(string str)
	{
		push_back(str);
	}
	void pop()
	{
		pop_back();
	}
	bool isEmpty()
	{
		return empty();
	}
	~MyStack()
	{}
};

#endif // !MYSTACK_H
