Source.cpp
--------------
#include<iostream>
#include<fstream>
#include"string"
#include"MyStack.h"

using namespace std;

int main()
{
	MyStack stack;
	ifstream read("read.txt");
	string str;
	bool result = true;
	while (!read.eof())
	{
		read >> str;
		if (str == "begin")
		{
			stack.push(str);
		}
		if (str == "end" || str == "end;")
		{
			if (stack.isEmpty() == true)
			{
				result = false;
				break;
			}
			else
			{
				stack.pop();
			}
		}
	}
	if (result == false || stack.isEmpty()==false)
	{
		cout << "code is wrong";
	}
	else
	{
		cout << "Everything is Ok";
	}
	read.close();
	system("pause");
}
