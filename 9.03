void Create(Node *Stack)
{
	Stack->count = 0;
	}
void push(Node *Stack, int j)
 {
	Stack->Array[Stack->count] = j;
	Stack->count++;

 }
int pop(Node *Stack)
{
	Stack->count--;
	return Stack->Array[Stack->count + 1];
}

